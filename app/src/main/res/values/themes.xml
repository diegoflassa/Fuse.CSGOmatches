<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.FuseCSGOmatches.Starting" parent="Theme.SplashScreen" tools:ignore="UnusedResources">
        <item name="android:statusBarColor">@color/launcher_color</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="windowSplashScreenBackground">@color/launcher_color</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_launcher
